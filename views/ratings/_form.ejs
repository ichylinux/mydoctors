<form action="/" method="POST">
  <div><label>病院の名前 <input type="text" name="hospitalName" /></label></div>
  <div><label>医者の名前 <input type="text" name="doctorName" /></label></div>
  <div>
    <span>評価</span>

    <% for (var i = 0; i < RATINGS.length; i ++) { %>
      <label><input type="radio" name="rating" value="<%= RATINGS[i][0] %>" /> <%= RATINGS[i][1] %></label>
      <span class="spacer"></span>
    <% } %>
  </div>
  <div><input type="submit" value="登録" /></div>
</form>
